{
  "name": "magic",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://qiangqiang-id.github.io/magic",
  "scripts": {
    "commit": "git-cz",
    "prepare": "node -r @babel/register scripts/prepare.js",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "npm run lint:es && npm run lint:style && npm run lint:prettier || exit 1",
    "lint:es": "eslint --ext .js,.ts,.jsx,.tsx ./ --cache",
    "lint:style": "stylelint \"**/*.{css,less}\" --cache",
    "lint-staged": "lint-staged",
    "lint:prettier": "prettier --list-different \"**/*.{ts,tsx,js,jsx,css,less,json,yml,yaml,md,mdx}\"",
    "commitlint": "commitlint --config commitlint.config.cjs -e -V",
    "fix": "npm run fix:es && npm run fix:style && npm run fix:prettier || exit 1",
    "fix:es": "eslint --ext .js,.ts,.jsx,.tsx ./ --cache --fix",
    "fix:style": "stylelint \"**/*.{css,less}\" --cache --fix",
    "fix:prettier": "prettier --write \"**/*.{ts,tsx,js,jsx,css,less,json,yml,yaml,md,mdx}\" --cache --fix"
  },
  "dependencies": {
    "@ant-design/icons": "^5.1.4",
    "@dnd-kit/core": "^6.0.8",
    "@dnd-kit/sortable": "^7.0.2",

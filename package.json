    "@vitejs/plugin-react": "^2.2.0",
    "babel": "^6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "commitizen": "^4.2.5",
    "cross-env": "^7.0.3",
    "cssnano": "^5.1.13",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.22.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
